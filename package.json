{
  "name": "filfa-url-redirector",
  "version": "0.0.1",
  "description": "Redirects requests from filfa.co to filterfacility.co.uk",
  "main": "lib/index.js",
  "scripts": {
    "start": "node lib/index.js",
    "build": "tsc",
    "predeploy": "npm run format && npm run fix && tsc",
    "dev": "tsc -w",
    "lint": "eslint src/{**/*,*}.{js,ts,jsx,tsx}",
    "fix": "eslint --fix src/{**/*,*}.{js,ts,jsx,tsx}",
    "format": "prettier --write src/{**/*,*}.{js,ts,jsx,tsx}",
    "test": "mocha test/*.test.js --check-leaks --timeout 5000"
  },
  "author": "Brian Evans",
  "license": "0BSD",
  "engines": {
    "node": ">=11.14"
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^15.0.3",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "7.25.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-promise": "5.1.0",
    "gaxios": "4.2.1",
    "mocha": "8.3.2",
    "prettier": "2.2.1",
    "typescript": "^4.2.4"
  }
}
